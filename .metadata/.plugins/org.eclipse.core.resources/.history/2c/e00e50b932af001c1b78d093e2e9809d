package com.chainsys.introduction;

/**
 * 
 * @author goku3159
 *
 */
public class Eight {

	public static void main(String[] args) {
		java.util.Scanner sc = new java.util.Scanner(System.in);
		String name = sc.nextLine();
		sc.close();
		char[] c = name.toCharArray();
		int n = c.length;
		for (int i = 0; i <= n; i++) {
			System.out.println(i);

		}

	}
}